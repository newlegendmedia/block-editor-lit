{
    "notion": {
      "type": "object",
      "key": "notion",
      "name": "Notion++",
      "properties": [
        { "type": "element", "key": "title", "name": "Title", "base": "text" },
        { "type": "element", "key": "createdOn", "name": "Created On", "ref": "metaDate" },
        { "type": "element", "key": "lastUpdated", "name": "Last Updated", "ref": "metaDate" },
        { "type": "element", "key": "tags", "name": "Tags", "base": "text" },
        { "type": "group", "key": "page", "name": "content", "ref": "page" }
      ]
    },
    "document": {
      "type": "object",
      "key": "document",
      "name": "Sample Document",
      "properties": [
        { "type": "element", "key": "title", "ref": "titleElement" },
        {
          "type": "array",
          "key": "sections",
          "name": "Document Sections",
          "itemType": { "type": "object", "key": "section", "ref": "sectionModel" },
          "repeatable": true
        },
        {
          "type": "group",
          "key": "predefinedGroup",
          "name": "Predefined Group",
          "editable": false,
          "itemTypes": [
            { "type": "element", "key": "date", "ref": "dateElement" },
            { "type": "object", "key": "author", "ref": "authorModel" }
          ]
        },
        {
          "type": "group",
          "key": "editableGroup",
          "name": "Editable Group",
          "editable": true,
          "itemTypes": { "type": "group", "ref": "editableGroupTypes" }
        }
      ]
    },
    "sectionModel": {
      "type": "object",
      "key": "sectionModel",
      "name": "Document Section",
      "properties": [
        { "name": "Heading", "type": "element", "key": "heading", "ref": "headingElement" },
        { "name": "Body", "type": "element", "key": "body", "ref": "bodyElement" }
      ]
    },
    "authorModel": {
      "type": "object",
      "key": "authorModel",
      "name": "Author",
      "properties": [
        { "name": "Name", "type": "element", "key": "name", "ref": "authorNameElement" },
        { "name": "Email", "type": "element", "key": "email", "ref": "authorEmailElement" }
      ]
    }    
  }